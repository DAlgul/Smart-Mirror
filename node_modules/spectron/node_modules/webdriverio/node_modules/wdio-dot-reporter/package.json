{
  "name": "wdio-dot-reporter",
  "version": "0.0.10",
  "description": "A WebdriverIO plugin. Report results in dot format.",
  "author": {
    "name": "Christian Bromann",
    "email": "christian@saucelabs.com"
  },
  "homepage": "https://github.com/webdriverio/wdio-dot-reporter#readme",
